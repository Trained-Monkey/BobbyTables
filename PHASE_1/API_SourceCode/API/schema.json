                    {
                        "required": true,
                        "schema": {
                            "title": "End Date",
                            "type": "string",
                            "format": "yyyy-MM-ddTHH:mm:ss"
                        },
                        "example": "2030-01-01T00:00:00",
                        "name": "end_date",
                        "in": "query"
                    },
                    {
                        "required": true,
                        "schema": {
                            "title": "Start Date",
                            "type": "string",
                            "format": "yyyy-MM-ddTHH:mm:ss"
                        },
                        "example": "2020-01-01T00:00:00",
                        "name": "start_date",
                        "in": "query"
                    },
                    {
                        "required": true,
                        "schema": {
                            "title": "Key Terms",
                            "type": "string"
                        },
                        "example": "outbreak",
                        "name": "key_terms",
                        "in": "query"
                    },
                    {
                        "required": true,
                        "schema": {
                            "title": "Location",
                            "type": "string"
                        },
                        "example": "Malawi",
                        "name": "location",
                        "in": "query"
                    },
                    {
                        "required": false,
                        "schema": {
                            "title": "Limit",
                            "type": "integer",
                            "default": 20
                        },
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "required": false,
                        "schema": {
                            "title": "Offset",
                            "type": "integer",
                            "default": 0
                        },
                        "name": "offset",
                        "in": "query"
                    },
                    {
                        "required": false,
                        "schema": {
                            "title": "Version",
                            "pattern": "^v[0-9]+\\.[0-9]+$",
                            "type": "string",
                            "default": "v1.0"
                        },
                        "name": "version",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ArticleList"
